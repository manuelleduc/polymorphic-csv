grammar polymorphic.Csv with org.eclipse.xtext.xbase.Xtype

generate csv "http://www.Csv.polymorphic"

Model:
	{Model}
	'package' name=ID ';'
	'constraints' '{'
	constraints+=Constraint*
	'}'
	'languages' '{'
	languages+=Language*
	'}'
	(actions+=Actions)*;

Constraint:
	name=ID '=' (true?='true' | 'false');

Language:
	name=ID '(' target=QualifiedName ')';

Actions:
	{OpenCSV} 'read' name=ID file=STRING charset=('latin1'|'utf8')|
	{PrintCSV} 'print' name=ID |
	{NbRow} 'nbrow' name=ID | // print number of rows (including header) // wc -l is certainly unbeatable  // TODO ideally I would like to store nbrow in a variable
	{SaveCSV} 'save' name=ID (file=STRING)?;